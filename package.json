{
  "name": "2004scape",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#jagex2/*": "./src/jagex2/*",
    "#lostcity/*": "./src/lostcity/*"
  },
  "scripts": {
    "start": "nodemon --ignore data/ --ignore dump/ src/lostcity/app.js",
    "cache:pack": "node src/lostcity/tools/pack/all.js",
    "script:symbols": "node src/lostcity/tools/scripts/symbols.js",
    "script:compile": "cd data && java -jar ../RuneScriptCompiler.jar src/scripts pack/server/scripts"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/static": "^6.10.2",
    "@jimp/png": "^0.22.8",
    "dotenv": "^16.3.1",
    "fastify": "^4.18.0",
    "jimp": "^0.22.8",
    "node-forge": "^1.3.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
