{
  "name": "2004scape",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "bun --watch run src/lostcity/app.ts",
    "web": "bun --watch run src/lostcity/web.ts",
    "client:pack": "bun run src/lostcity/tools/client/all/pack.ts",
    "client:clean": "bun run src/lostcity/tools/client/clean.ts",
    "script:symbols": "bun run src/lostcity/tools/server/scripts/symbols.js",
    "script:compile": "cd data && java -jar ../RuneScriptCompiler.jar src/scripts pack/server",
    "script:run": "bun run src/lostcity/tools/server/runscript.ts",
    "server:pack": "bun run src/lostcity/tools/server/all/pack.ts",
    "server:clean": "bun run src/lostcity/tools/server/clean.ts",
    "server:build": "bun run server:pack && bun run script:symbols && bun run script:compile",
    "lint": "eslint . --ext .ts --ext .js"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/session": "^10.5.0",
    "@fastify/static": "^6.11.2",
    "@fastify/view": "^8.2.0",
    "@jimp/png": "^0.22.10",
    "ejs": "^3.1.9",
    "fastify": "^4.23.2",
    "jimp": "^0.22.10",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.6.3",
    "@types/node-forge": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "bun-types": "^1.0.7",
    "eslint": "^8.49.0"
  }
}
