[proc,fib](int $n)(int)
if ($n <= 1) return($n);
return(calc(~fib($n - 2) + ~fib($n - 1)));

[proc,fib2](int $n)(int)
def_int $i = 0;
def_int $num1 = 0;
def_int $num2 = 1;
while ($i < $n) {
    $num1, $num2 = $num2, calc($num1 + $num2);
    $i = calc($i + 1);
}
return($num1);

[proc,gosubtest]
~fib(30);

[proc,looptest]
~fib2(30);
